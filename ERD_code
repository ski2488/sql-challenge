departments
-
dept_no PK varchar(4)
dept_name varchar

dept_emp
-
emp_no int PK
dept_no varchar(4) PK

dept_manager
-
dept_no varchar(4) FK >- departments.dept_no
emp_no PK int

employees
-
emp_no PK int
emp_title_id varchar(5) FK >- titles.title_id
birth_date date
first_name varchar
last_name varchar
sex varchar(1)
hire_date date

salaries
-
emp_no PK int
salary int

titles
-
title_id PK varchar(5)
title varchar